/* skip navigation code borrowed from jim thatcher - http://jimthatcher.com/skipnav.htm */
#skipnav {}
#skipnav a {
	position:absolute;
	z-index: 100;
	left:-1000px;
	top:0px;
	width:1px;
	height:1px;
	overflow:hidden;
}

#skipnav a:active,
#skipnav a:focus {
	position: absolute;
	display: block;
	left: auto;
	top: 0px;
	font-size: 15px;
	font-weight: bold;
	width: 150px;
	height: 24px;
	padding: 3px 12px;
	border: 1px solid #e9dfb0;
	background: #fcfeff;
	overflow: visible;
	text-decoration: underline;
}

