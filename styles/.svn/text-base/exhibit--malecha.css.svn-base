div.views-field-field-caption-value{
	position: absolute;
	top: 570px;
	font-size: 11px !important;
}
.views-slideshow-controls-bottom{
	float: right;
	 margin: 0 22px 0 0;
}
.views_slideshow_controls_text_pause{
    display: block;
    height: 0;
    padding-top: 30px;
    width: 3px;
    float: left;
    background: #666666;
    margin: 0 13px 0 10px;
    cursor: default;
    text-indent: 10000px;
}
.views_slideshow_controls_text_play{
    display: block;
    height: 0;
    padding-top: 30px;
    width: 3px;
    float: left;
    background: #666666;
    margin: 0 13px 0 10px;
    cursor: default;
    text-indent: 10000px;
}
.views_slideshow_controls_text_previous{
    background: url("/sites/default/files/images/previous.png") no-repeat scroll 0 0 transparent;
    display: block;
    height: 0;
    overflow: hidden;
    width: 40px;
    float: left;
    padding-top: 40px;
}
.views_slideshow_controls_text_next{
    background: url("/sites/default/files/images/next.png") no-repeat scroll 0 0px transparent;
    display: block;
    height: 0;
    overflow: hidden;
    width: 60px;
    padding-top: 40px;
}
div#page {
    overflow: hidden;}