h1 {
	border-bottom: none !important;
}
h3 a{
	border-bottom: none !important;
}
th {
	background-color: #DDD;
}
#accordion h4{
	color: #555;
	font-size: 15px !important;
	line-height: 16px !important;
}
#block-block-48 {
	background-image: url('/sites/default/files/chat_bubbles.png');
	height: 192px;
}
#block-block-48 h2{
	padding-left: 10px;
	padding-top: 40px;
	text-align: center;
	width: 200px;
}
#block-block-49, #block-block-66{
	border: 1px dotted #CCC;
	padding-bottom: 5px;
}
#block-block-66 {
	padding: 5px;
	overflow: auto;
}
#block-block-66 a {
	color: #004276;
	font-weight: 700;
}
#block-block-66 img {
	margin: 0 7px 0 0;
	float: left;
}
#block-block-49 h2{
	border-bottom: 1px solid #DDD;
	color: #004276;
	font-size: 18px;
	margin-bottom: 5px;
	margin-left: 5px;
	margin-right: 5px;
	padding-bottom: 10px !important;
}
#block-block-49 p{
	background-color: #DDD;
	border: 1px solid #4C83AF;
	font-size: 11px !important;
	margin-bottom: 0px !important;
	margin-left: 5px !important;
	margin-right: 5px !important;
	padding: 5px;
}
#block-block-49 p strong{
	color: #004276;
}
#block-block-51, #block-block-65 {
	margin: 54px 0 0 0;
	padding: 5px;
	border: 1px dotted #CCC;
	width: 185px;
}
#block-block-65 {
	margin-top: 10px;
}
#block-block-51 span.askus, #block-block-65 p.text{
	display:block;
	font-weight:700;
	margin-bottom:5px;
	background-color: #DDD;
	border: 1px solid #4C83AF;
	padding: 5px;
	color:#004276;
}
#block-block-51 a.askusklink{
	color: white;
	background: #C00;
	padding: 2px;
	display:inline-block;
}
#block-block-51 a:hover{
	text-decoration:none;
}
#page p.book-count {
	font-size: 300%;
	line-height: 1;
	text-align: center;
	font-weight: 700;
}
#callout{
	width: 135px;
	height: 46px;
	margin: 11px 12px 60px 31px;
	text-align: center;
	padding: 9px 16px 10px 16px;
	display: block;
	float: right;
	background: url("/sites/default/files/css/parens.jpg") no-repeat;
	margin-top: 22px;
}
#page #callout h2 {
	margin: 0;
	padding: 0;
}
#page #callout ul {
margin: 0;
padding: 2px 0 0 0
}
#page #callout ul li{
	background: none;
	margin: 0;
	padding: 0;
	font-weight: 700;
}
#sidebar-left {
	margin-top: 20px;
}
#sidebar-right {
	margin-top: 20px;
}
.boundtitle {
	border-bottom: 1px solid #DDD;
	font-size: 21px !important;
	font-weight: bold;
	line-height: 24px !important;
	padding: 7px 0 3px 0 !important;
}
.chatbubbletext {
	margin-left: 20px !important;
	text-align: center;
	width: 180px;
}

.ui-corner-all {
	moz-border-radius: 0px;
	-webkit-border-radius: 0px;
	border-radius: 0px;
}
.ui-corner-bottom {
	moz-border-radius: 0px;
	-webkit-border-radius: 0px;
	border-radius: 0px;
}
.ui-corner-top {
	moz-border-radius: 0px;
	-webkit-border-radius: 0px;
	border-radius: 0px;
}
.ui-state-default {
	background: none;
	/*border: none;*/
}
.ui-widget {
	font-family: Arial, Helvetica, sans-serif !important;
}
.ui-widget-content {
	color: #555;
}
